{
    "name": "nijat/ase",
    "description": "The ASE Project.",
    "keywords": ["ase"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "arrilot/laravel-widgets": "^3.6",
        "artesaos/seotools": "^0.10.0",
        "barryvdh/laravel-debugbar": "^v3.4.2",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-translation-manager": "^v0.5.10",
        "berkayk/onesignal-laravel": "^1.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.2",
        "dimsav/laravel-translatable": "^v9.5.0",
        "doctrine/dbal": "^2.5",
        "dompdf/dompdf": "^0.8.3",
        "fabpot/goutte": "^4.0",
        "intervention/image": "^2.4",
        "ionutmilica/laravel-settings": "^2.0"  ,
        "irazasyed/telegram-bot-sdk": "^2.0",
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "laravel-notification-channels/onesignal": "^2.3",
        "laravel/framework": "5.7.*",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "maatwebsite/excel": "3.1.*",
        "milon/barcode": "6.0.5",
        "mpdf/mpdf": "^8.0",
        "nicat/static-map": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "rap2hpoutre/laravel-log-viewer": "^0.7.0",
        "rmccue/requests": "^1.7",
        "santigarcor/laratrust": "5.1.*",
        "tightenco/quicksand": "^2.0",
        "tymon/jwt-auth": "0.5.*",
        "uxweb/sweet-alert": "~1.4",
        "watson/rememberable": "^2.0",
        "swiftmailer/swiftmailer": "~6.0"
    },
    "require-dev": {
        "filp/whoops" : "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "laravel/homestead": "^6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan config:clear",
            "php artisan route:clear",
            "php artisan view:clear",
            "php artisan cache:clear"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan config:clear",
            "php artisan route:clear",
            "php artisan view:clear",
            "php artisan cache:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
